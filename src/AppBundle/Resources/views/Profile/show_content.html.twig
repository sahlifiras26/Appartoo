{% trans_default_domain 'FOSUserBundle' %}

<div class="fos_user_user_show">
    <p>{{ 'username'|trans }}: {{ user.username }}</p>
    <p>{{ 'email'|trans }}: {{ user.email }}</p>
     <p>{{ 'couleur'|trans }}: {{ user.couleur }}</p>
      <p>{{ 'famille'|trans }}: {{ user.famille }}</p>
       <p>{{ 'nourriture'|trans }}: {{ user.nourriture }}</p>
        <p>{{ 'age'|trans }}: {{ user.age }}</p>
</div>
